{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "blog_content": {
      "type": "dynamiczone",
      "components": [
        "blog.video-block",
        "blog.text-image",
        "blog.text-block"
      ]
    },
    "blog_status": {
      "type": "enumeration",
      "required": true,
      "default": "draft",
      "enum": [
        "draft",
        "reviewed"
      ]
    }
  }
}
